<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Theme">
        <attr name="colorPrimary" format="color" />
        <attr name="colorNewPrimary" format="color" />
        <attr name="colorAccent" format="color" />
        <attr name="colorOnAccent" format="color" />
        <attr name="colorToolbar" format="color" />
        <attr name="colorToolbarItem" format="color" />
        <attr name="colorToolbarItemSecondary" format="color" />
        <attr name="colorToolbarItemActive" format="color" />
        <attr name="colorToolbarSurface" format="color" />
        <attr name="colorOnToolbarSurface" format="color" />
        <attr name="colorNavBarSurface" format="color" />
        <attr name="colorOnNavBarSurface" format="color" />
        <attr name="colorText" format="color" />
        <attr name="colorTextOnPrimary" format="color" />
        <attr name="colorTextSecondary" format="color" />
        <attr name="color_text_disabled" format="color" />
        <attr name="colorBackground" format="color" />
        <attr name="colorWindowBackground" format="color" />
        <attr name="colorChip" format="color" />
        <attr name="colorOnChip" format="color" />
        <attr name="colorUnselected" format="color" />
        <attr name="colorNavBar" format="color" />
        <attr name="colorFloorCard" format="color" />
        <attr name="colorCard" format="color" />
        <attr name="colorDivider" format="color" />
        <attr name="shadow_color" format="color" />
        <attr name="colorIndicator" format="color" />
        <attr name="colorPlaceholder" format="color" />
    </declare-styleable>

    <declare-styleable name="ShadowLayout">
        <attr name="shadow_layout_color" format="color" />
        <attr name="shadow_layout_background_color" format="color" />
        <attr name="shadow_layout_offsetX" format="dimension" />
        <attr name="shadow_layout_offsetY" format="dimension" />
        <attr name="shadow_layout_blur" format="dimension" />
        <attr name="shadow_layout_radius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="TintTabLayout">
        <attr name="layoutBackground" format="color" />
        <attr name="tabTextColor" format="color" />
        <attr name="tabIconTint" format="color" />
        <attr name="tabSelectedTextColor" format="color" />
        <attr name="tabIndicatorColor" />
    </declare-styleable>

    <declare-styleable name="TintMaterialRadioButton">
        <attr name="backgroundTint" />
        <attr name="radioButtonTint" format="color" />
        <attr name="tint" />
    </declare-styleable>

    <declare-styleable name="LoadMoreFooter">
        <attr name="backgroundTint" />
        <attr name="radius" format="dimension" />
        <attr name="footerSpinnerStyle" format="enum">
            <enum name="scale" value="0" />
            <enum name="translate" value="1" />
            <enum name="fixedBehind" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="TintView">
        <attr name="backgroundTint" format="color" />
        <attr name="tint" format="color" />
        <attr name="tintList" format="color" />
    </declare-styleable>

    <declare-styleable name="TintRecyclerView">
        <attr name="backgroundTint" />
    </declare-styleable>

    <declare-styleable name="TintImageView">
        <attr name="backgroundTint" />
        <attr name="tint" />
    </declare-styleable>

    <declare-styleable name="TintNavigationView">
        <attr name="navigationBackgroundTint" format="color" />
        <attr name="itemIconTint" format="color" />
        <attr name="itemTextTint" format="color" />
    </declare-styleable>

    <declare-styleable name="TintBottomNavigationView">
        <attr name="bottomNavigationBackgroundTint" format="color" />
        <attr name="itemIconTintList" format="color" />
        <attr name="itemTextTintList" format="color" />
    </declare-styleable>

    <declare-styleable name="TintNavigationRailView">
        <attr name="backgroundTint" />
        <attr name="itemIconTintList" />
        <attr name="itemTextTintList" />
    </declare-styleable>

    <declare-styleable name="TintToolbar">
        <attr name="toolbarBackgroundTint" format="color" />
        <attr name="itemTint" format="color" />
        <attr name="secondaryItemTint" format="color" />
        <attr name="activeItemTint" format="color" />
    </declare-styleable>

    <declare-styleable name="MyToolbar">
        <attr name="navigationButtonGravity">
            <flag name="center_vertical" value="0x10" />
            <flag name="top" value="0x30" />
            <flag name="bottom" value="0x50" />
        </attr>
    </declare-styleable>

    <declare-styleable name="TintSwitch">
        <attr name="switchBackgroundTint" format="color" />
        <attr name="thumbTint" format="color" />
        <attr name="trackTintList" format="color" />
        <attr name="textColor" />
    </declare-styleable>

    <declare-styleable name="TintMaterialCardView">
        <attr name="materialCardBackgroundTint" format="color" />
        <attr name="strokeColor" format="color" />
    </declare-styleable>

    <declare-styleable name="TintSeekbar">
        <attr name="seekbarBackgroundTint" format="color" />
        <attr name="progressTint" format="color" />
        <attr name="progressBackgroundTint" format="color" />
        <attr name="thumbColor" />
    </declare-styleable>

    <declare-styleable name="TintMaterialButton">
        <attr name="buttonBackgroundTint" format="color" />
        <attr name="buttonTextColor" format="color" />
        <attr name="buttonStrokeColor" format="color" />
    </declare-styleable>

    <declare-styleable name="TintCheckBox">
        <attr name="checkboxBackgroundTint" format="color" />
        <attr name="buttonTint" format="color" />
        <attr name="textColor" format="color" />
    </declare-styleable>

    <declare-styleable name="TintTextInputLayout">
        <attr name="boxBackgroundColor" />
        <attr name="boxStrokeColor" />
        <attr name="hintTextColor" />
        <attr name="placeholderTextColor" />
    </declare-styleable>

    <declare-styleable name="TintUndoableEditText">
        <attr name="cursorColor" format="color" />
        <attr name="hintTextColor" format="color" />
        <attr name="textColor" />
    </declare-styleable>

    <declare-styleable name="MyViewPager">
        <attr name="orientation" />
        <attr name="can_scroll" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TintEditText">
        <attr name="textColor" />
        <attr name="android:textColorHint" />
    </declare-styleable>

    <declare-styleable name="TintTextInputEditText">
        <attr name="textColor" />
        <attr name="textColorHint" format="color" />
    </declare-styleable>

    <declare-styleable name="TintCollapsingToolbarLayout">
        <attr name="textColor" />
    </declare-styleable>

    <declare-styleable name="RulerSeekBar">
        <attr name="rulerColor" format="color" />
        <attr name="rulerSize" format="dimension" />
        <attr name="rulerCount" format="integer" />
        <attr name="rulerShowTopOfThumb" format="boolean" />
        <attr name="rulerOnEdge" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ScrollCollapsingToolbarLayout">
        <!--  Specifies extra space on the start, top, end and bottom
              sides of the the expanded title text. Margin values should be positive. -->
        <attr name="expandedTitleMargin" />
        <!--  Specifies extra space on the start side of the the expanded title text.
              Margin values should be positive. -->
        <attr name="expandedTitleMarginStart" />
        <!--  Specifies extra space on the top side of the the expanded title text.
              Margin values should be positive. -->
        <attr name="expandedTitleMarginTop" />
        <!--  Specifies extra space on the end side of the the expanded title text.
              Margin values should be positive. -->
        <attr name="expandedTitleMarginEnd" />
        <!--  Specifies extra space on the bottom side of the the expanded title text.
              Margin values should be positive. -->
        <attr name="expandedTitleMarginBottom" />
        <!-- The text appearance of the CollapsingToolbarLayout's title when it is fully
             'expanded' -->
        <attr name="expandedTitleTextAppearance" />
        <!-- The text appearance of the CollapsingToolbarLayouts title when it is fully
             'collapsed' -->
        <attr name="collapsedTitleTextAppearance" />
        <!-- The drawable to use as a scrim on top of the CollapsingToolbarLayouts content when
             it has been scrolled sufficiently off screen. -->
        <attr name="contentScrim" />
        <!-- The drawable to use as a scrim for the status bar content when the
             CollapsingToolbarLayout has been scrolled sufficiently off screen. Only works on
             Lollipop when used together with android:fitSystemWindows="true". -->
        <attr name="statusBarScrim" />
        <!-- The id of the primary Toolbar child that you wish to use for the purpose of collapsing.
             This Toolbar descendant view does not need to be a direct child of the layout.
             If you do not set this, the first direct Toolbar child found will be used. -->
        <attr name="toolbarId" />
        <!-- Specifies the amount of visible height in pixels used to define when to trigger a
             scrim visibility change. -->
        <attr name="scrimVisibleHeightTrigger" />
        <!-- Specifies the duration used for scrim visibility animations. -->
        <attr name="scrimAnimationDuration" />

        <!-- Specifies how the title should be positioned when collapsed. -->
        <attr name="collapsedTitleGravity" />

        <!-- Specifies how the title should be positioned when expanded. -->
        <attr name="expandedTitleGravity" />

        <!-- Whether the CollapsingToolbarLayout should draw its own shrinking/growing title. -->
        <attr name="titleEnabled" />
        <!-- The title to show when titleEnabled is set to true. -->
        <attr name="title" />
        <!-- The maximum number of lines to display in the expanded state. Experimental Feature. -->
        <attr name="maxLines" />
    </declare-styleable>
    <declare-styleable name="ScrollCollapsingToolbarLayout_Layout">
        <attr name="collapseMode">
            <!-- The view will act as normal with no collapsing behavior. -->
            <enum name="none" value="0" />
            <!-- The view will pin in place. -->
            <enum name="pin" value="1" />
            <!-- The view will scroll in a parallax fashion. See the
                 layout_collapseParallaxMultiplier attribute to change the multiplier. -->
            <enum name="parallax" value="2" />
            <enum name="scroll" value="3" />
        </attr>

        <attr name="collapseParallaxMultiplier" format="float" />

        <attr name="collapsedGravity">
            <flag name="top" value="0x30" />
            <flag name="bottom" value="0x50" />
            <flag name="left" value="0x03" />
            <flag name="right" value="0x05" />
            <flag name="center_vertical" value="0x10" />
            <flag name="fill_vertical" value="0x70" />
            <flag name="center_horizontal" value="0x01" />
            <flag name="center" value="0x11" />
            <flag name="start" value="0x00800003" />
            <flag name="end" value="0x00800005" />
        </attr>

        <attr name="collapsedMargin" format="dimension" />
        <attr name="collapsedMarginStart" format="dimension" />
        <attr name="collapsedMarginTop" format="dimension" />
        <attr name="collapsedMarginEnd" format="dimension" />
        <attr name="collapsedMarginBottom" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CircleProgressView">
        <attr name="progressBackgroundColor" format="color" />
        <attr name="progressColor" format="color" />
        <attr name="progressTextColor" format="color" />
        <attr name="showProgressText" format="boolean" />
        <attr name="progressTextSize" format="dimension" />
        <attr name="progressWidth" format="dimension" />
        <attr name="progress" format="integer" />
    </declare-styleable>

    <declare-styleable name="TintCircleProgressView">
        <attr name="progressBackgroundColor" />
        <attr name="progressColor" />
        <attr name="progressTextColor" />
    </declare-styleable>
</resources>